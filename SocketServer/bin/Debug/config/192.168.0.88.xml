<?xml version="1.0" encoding="utf-8"?>
<!-- attempts_interval 定义值，不可随意设置（最少0.5s）：                
    1S     = 1,
    2S     = 2,    
 -->
<!-- device protocol：                
    modbus  ,
    dlt645-07     = 07协议,
    dlt645-97     = 97协议,    
 -->
<collector>
  <attr scada_frequency="2" attempts_num="3" attempts_interval="3" />
  <!-- serial：                
    modify:baude bits parity stop
		yes	,可修改
		no  ,不可修改
 -->
  <serial>
    <para protocol="dlt645" port="/dev/ttyS8" baude="1200" bits="8" parity="E" stop="1" resume_interval="5" detect_count="3" modify="yes" />
    <para protocol="dlt645" port="/dev/ttyS8" baude="1200" bits="8" parity="E" stop="1" resume_interval="5" detect_count="3" modify="yes" />
  </serial>
  <!--
	modbus：
		type:float=1  long=2 
        DIid： DI0 DI1  
    -->
  <device name="dlt645_dev1" protocol="dlt645" devmodel="dlt645-97" address="041008309980" serialport="8" actstate="0" interval="3">
    <tag name="M_CYC_D_JJZI_E" DIid="B650" id="1" type="1" />
    <tag name="M_CYC_D_JJZI_EF_PM" DIid="9010" id="2" type="1" />
    <tag name="M_CYC_D_JJZI_ER_PM" DIid="9020" id="3" type="1" />
  </device>
  <device name="dlt645_dev1" protocol="dlt645" devmodel="dlt645-97" address="041008309980" serialport="8" actstate="0" interval="3">
    <tag name="M_CYC_D_JJZII_E" DIid="B650" id="4" />
    <tag name="M_CYC_D_JJZII_EF_PM" DIid="9010" id="5" />
    <tag name="M_CYC_D_JJZII_ER_PM" DIid="9020" id="6" />
  </device>
</collector>